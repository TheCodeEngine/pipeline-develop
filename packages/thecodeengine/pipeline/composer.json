{
    "name": "thecodeengine/pipeline",
    "type": "library",
    "description": "Laravel package for command pipelines",
    "keywords": [
        "thecodeengine",
        "pipeline"
    ],
    "homepage": "https://github.com/thecodeengine/pipeline",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Stoldt",
            "email": "konstantin.stoldt@gmail.com",
            "homepage": "https://github.com/thecodeengine",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "TheCodeEngine\\Pipeline\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheCodeEngine\\Pipeline\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
